{
  "name": "millie-exams-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt --port 3008",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "dev:sls": "npm run build && sls offline start",
    "deploy": "npm run build && rm -rf node_modules && yarn install --production --frozen-lockfile && npm i -g serverless@2.8.0 serverless-apigw-binary@0.4.4 serverless-domain-manager@5.0.0 serverless-dotenv-plugin@3.1.0 serverless-offline@6.8.0 serverless-plugin-tracing@2.0.0 serverless-plugin-warmup@4.9.0 serverless-s3-sync@1.15.0 && sls deploy",
    "domain": "serverless create_domain",
    "rdomain": "serverless delete_domain",
    "deploy:credentials": "serverless config credentials"
  },
  "dependencies": {
    "@nuxt/components": "^1.1.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/sentry": "^5.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "google-libphonenumber": "^3.2.17",
    "js-file-download": "^0.4.12",
    "json2csv": "^5.0.6",
    "jszip": "^3.6.0",
    "lodash": "^4.17.20",
    "node-sass": "^8.0.0",
    "nuxt-start": "^2.14.7",
    "portal-vue": "^2.1.7",
    "sass-loader": "^9.0.3",
    "v-click-outside": "^3.1.2",
    "vue2-datepicker": "^3.8.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "nuxt": "^2.14.6",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "^7",
    "tailwindcss": "^3.1.8"
  }
